# Variables de entorno necesarias para el proyecto
# Copia este archivo como .env y completa con tus valores

# Base de datos MySQL
DB_HOST=localhost
DB_PORT=3306
DB_NAME=boutique_uniformes
DB_USERNAME=root
DB_PASSWORD=your_password

# OAuth2 Google
GOOGLE_CLIENT_ID=your-google-client-id
